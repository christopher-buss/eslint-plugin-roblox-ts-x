{
	"name": "eslint-plugin-roblox-ts",
	"version": "1.0.0",
	"description": "A collection of ESLint rules specifically targeted for roblox-ts.",
	"keywords": [
		"roblox",
		"roblox-ts",
		"eslint-plugin",
		"rbxts"
	],
	"homepage": "https://github.com/roblox-ts/eslint-plugin-roblox-ts#readme",
	"bugs": "https://github.com/roblox-ts/eslint-plugin-roblox-ts/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/roblox-ts/eslint-plugin-roblox-ts.git"
	},
	"license": "MIT",
	"author": "Christopher Buss <christopher.buss@pm.me> (https://github.com/christopher-buss)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"create-rule": "tsx scripts/create-rule.ts",
		"dev": "unbuild --stub",
		"eslint-docs": "eslint-doc-generator",
		"lint": "eslint",
		"prepare": "simple-git-hooks",
		"prepublishOnly": "nr build",
		"release": "bumpp",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
	},
	"lint-staged": {
		"*": "eslint --fix"
	},
	"resolutions": {
		"eslint-plugin-roblox-ts": "workspace:*"
	},
	"devDependencies": {
		"@antfu/ni": "catalog:dev",
		"@isentinel/eslint-config": "catalog:dev",
		"@rbxts/compiler-types": "catalog:types",
		"@rbxts/types": "catalog:types",
		"@types/node": "catalog:types",
		"@typescript-eslint/eslint-plugin": "catalog:dev",
		"@typescript-eslint/parser": "catalog:dev",
		"@typescript-eslint/type-utils": "catalog:dev",
		"@typescript-eslint/utils": "catalog:dev",
		"bumpp": "catalog:dev",
		"eslint": "catalog:dev",
		"eslint-doc-generator": "catalog:utils",
		"eslint-plugin-eslint-plugin": "catalog:testing",
		"eslint-plugin-pnpm": "catalog:dev",
		"eslint-vitest-rule-tester": "catalog:testing",
		"jiti": "catalog:dev",
		"lint-staged": "catalog:dev",
		"simple-git-hooks": "catalog:dev",
		"ts-api-utils": "catalog:dev",
		"tsx": "catalog:dev",
		"typescript": "catalog:dev",
		"unbuild": "catalog:dev",
		"vitest": "catalog:testing",
		"yaml": "catalog:testing"
	},
	"peerDependencies": {
		"eslint": "*"
	},
	"packageManager": "pnpm@10.10.0"
}
